     "execution_count": 3,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "\n",
    "import numpy as np\n",
    "\n",
    "A = np.random.randint(0, 11, size=(4, 4))\n",
    "\n",
    "# np.linalg\n",
    "\n",
    "Ainv = np.linalg.inv(A)\n",
    "\n",
    "np.allclose(np.matmul(A, Ainv), np.identity(4))"
   ]
  },
